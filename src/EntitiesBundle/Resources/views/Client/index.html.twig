{% extends 'AppBundle::base.html.twig' %}

{% block header %}{% endblock %}

{% block stylesheets %}
    {% stylesheets filter="scssphp" output="assets/entities/client/index/css/main.css"
        '@AppBundle/Resources/public/scss/select-box.scss'
    %}
        <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
{% endblock %}

{% block body_container %}
    <div class="js_dataBox"></div>
{% endblock %}

{% block local_javascripts %}
    {% javascripts output="assets/entities/client/index/js/main.js"
        '@EntitiesBundle/Resources/public/client/index/webpack_generated/common.js'
        '@EntitiesBundle/Resources/public/client/index/webpack_generated/app.js'
        '@EntitiesBundle/Resources/public/client/index/webpack_generated/main.js'
    %}
        <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}