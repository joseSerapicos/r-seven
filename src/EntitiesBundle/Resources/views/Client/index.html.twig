{% extends 'AppBundle::base.html.twig' %}

{% block header %}{% endblock %}

{% block stylesheets %}
    {% stylesheets filter="scssphp" output="assets/entities/client/index/css/main.css"
        '@AppBundle/Resources/public/scss/select-box.scss'
    %}
        <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
{% endblock %}

{% block body_container %}
    <div class="js_appBasics"></div>
    <div class="js_dataBox"></div>
{% endblock %}

{%  block local_javascripts %}
    {% javascripts output="assets/entities/client/index/js/local.js"
        '@EntitiesBundle/Resources/public/client/index/ts/dist/inline.bundle.js'
        '@EntitiesBundle/Resources/public/client/index/ts/dist/polyfills.bundle.js'
        '@EntitiesBundle/Resources/public/client/index/ts/dist/vendor.bundle.js'
        '@EntitiesBundle/Resources/public/client/index/ts/dist/main.bundle.js'
    %}
