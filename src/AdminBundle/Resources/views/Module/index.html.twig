{% extends 'AppBundle:tree-view:index.html.twig' %}

{% block stylesheets %}
    {{ parent() }}

    {% stylesheets output="assets/admin/module/index/css/local.css"
    '@AdminBundle/Resources/public/module/index/scss/dist/styles.css'
    %}
    <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
{% endblock %}

{% block javascripts %}
    {% javascripts output="assets/admin/module/index/js/local.js"
        '@AdminBundle/Resources/public/module/index/ts/dist/inline.bundle.js'
        '@AdminBundle/Resources/public/module/index/ts/dist/polyfills.bundle.js'
        '@AdminBundle/Resources/public/module/index/ts/dist/vendor.bundle.js'
        '@AdminBundle/Resources/public/module/index/ts/dist/main.bundle.js'
    %}
        <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}