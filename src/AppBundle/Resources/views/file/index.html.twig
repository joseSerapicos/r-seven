{% extends 'AppBundle::base.html.twig' %}

{% block stylesheets %}
    {% stylesheets filter="scssphp" output="assets/app/file/css/all.css"
        '@dropzone_css'
    %}
        <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
{% endblock %}

{% block header %}{% endblock %}

{% block body_container %}
    <div class="js_file"></div>
{% endblock %}

{% block local_javascripts %}
    {% javascripts output="assets/app/file/js/local.js"
        '@AppBundle/Resources/public/file/webpack_generated/common.js'
        '@AppBundle/Resources/public/file/webpack_generated/app.js'
        '@AppBundle/Resources/public/file/webpack_generated/main.js'
    %}
        <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}