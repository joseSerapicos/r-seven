{% extends 'AppBundle:data-box:base.html.twig' %}
{% block body_container %}
    <div class="border rounded"><div
                class="bg-light d-inline-block m-2 p-2"
                *ngFor="let obj of _dataService.getProviderAttr('objects'); let objIndex = index"><span
                    class="align-middle mr-1"
                    [innerHtml]="obj['name']"></span><a
                    class="action fa fa-times"
                    (click)="deleteAction($event, objIndex)"></a>
    </div></div>
{% endblock %}
{% block footer %}{% endblock %}