{% extends 'AppBundle:tree-view:popup.html.twig' %}

{% block footer_container %}
    <div class="form-horizontal">
        <div class="form-group">
            <div class="col-sm-10 col-sm-offset-2">
                <button class="btn-default btn" type="button" (click)="cancelAction($event)">Cancel</button>
                <button class="btn-primary btn" type="button" (click)="saveAndCloseAction($event)">Save and Close</button>
            </div>
        </div>
    </div>
{% endblock %}