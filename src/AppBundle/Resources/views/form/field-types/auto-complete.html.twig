{% set isRequired = (isRequired is defined ? isRequired : true) %}
{% if (field is defined) and (formField is defined) %}
    <input [(ngModel)]="_formService.getObject().{{field}}"
           formControlName = {{field}}
           name="{{formField.vars.full_name}}"
           {% if (formField.vars.required and isRequired) %}required="required"{% endif %}
           type="hidden">
    <js_autoComplete [field]="'{{field}}'"
                     [hasSelfReference]="_dataService.getFieldChoicesAttr('{{ field }}', 'hasSelfReference')"
                     {% if (formField.vars.attr is defined) %}
                         {% for attr, value in formField.vars.attr %}{{attr}}="{{value}}"{% endfor %}
                     {% endif %}
    ></js_autoComplete>
    {% do formField.setRendered %}
{% endif %}