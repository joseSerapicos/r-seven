{% extends 'AppBundle:tree-view:index.html.twig' %}

{% block stylesheets %}
    {{ parent() }}

    {% stylesheets filter="scssphp" output="assets/sysadmin/module/index/css/local.css"
        '@AppBundle/Resources/public/scss/select-box.scss'
    %}
        <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
{% endblock %}

{% block local_javascripts %}
    {% javascripts output="assets/sysadmin/module/index/js/local.js"
        '@SysadminBundle/Resources/public/module/index/ts/dist/inline.bundle.js'
        '@SysadminBundle/Resources/public/module/index/ts/dist/polyfills.bundle.js'
        '@SysadminBundle/Resources/public/module/index/ts/dist/vendor.bundle.js'
        '@SysadminBundle/Resources/public/module/index/ts/dist/main.bundle.js'
    %}
        <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}